{
    "name": "firehed/u2f",
    "description": "A library providing U2F authentication",
    "license": "MIT",
    "keywords": [
        "auth",
        "authentication",
        "mfa",
        "security",
        "u2f",
        "webauthn",
        "web authentication",
        "webauthentication",
        "yubico",
        "yubikey"
    ],
    "homepage": "https://github.com/Firehed/u2f-php",
    "require": {
        "php": ">=7.2",
        "firehed/cbor": "dev-master"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^6.0 || ^7.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Firehed\\U2F\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Firehed\\U2F\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eric Stern",
            "email": "eric@ericstern.com"
        }
    ]
}
